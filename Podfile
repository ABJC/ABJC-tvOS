# Uncomment the next line to define a global platform for your project
platform :tvos, '15.0'
project 'ABJC.xcodeproj'

target 'ABJC tvOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # VLCKit
  pod 'TVVLCKit', '~>3.3.0'

  # SwiftLint & SwiftFormat
  pod 'SwiftLint'
  pod 'SwiftFormat/CLI'
end
